<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kantczak.imagedrawproject">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- <activity android:name="grabCutAlgorithm" /> -->
        <activity
            android:name=".NewMaskActivity"
            android:label="@string/title_activity_new_mask"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AddTagsActivity"
            android:label="@string/title_activity_add_tags"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
